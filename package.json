{
  "name": "agentflow-vscode",
  "repository": {
    "type": "git",
    "url": "https://github.com/omniaura/agentflow-vscode.git"
  },
  "displayName": "AgentFlow",
  "description": "Syntax Highlighting for AgentFlow Prompting Language",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "agentflow",
        "aliases": [
          "AgentFlow",
          "agentflow"
        ],
        "extensions": [
          ".af"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "agentflow",
        "scopeName": "source.af",
        "path": "./syntaxes/agentflow.tmLanguage.json"
      }
    ]
  }
}