{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "AgentFlow",
	"patterns": [
		{
			"include": "#title"
		},
		{
			"include": "#variable"
		}
	],
	"repository": {
		"title": {
			"match": "^(\\.title)\\s+(.*)$",
			"captures": {
				"1": {
					"name": "keyword.control.agentflow"
				},
				"2": {
					"name": "entity.name.function.agentflow"
				}
			}
		},
		"variable": {
			"match": "<!(\\w+)>",
			"captures": {
				"0": {
					"name": "variable.other.agentflow"
				}
			}
		}
	},
	"scopeName": "source.af"
}